    <mesh class="go1" file="trunk.stl"/>
    <mesh class="go1" file="hip.stl"/>
    <mesh class="go1" file="thigh_mirror.stl"/>
    <mesh class="go1" file="calf.stl"/>
    <mesh class="go1" file="thigh.stl"/>
